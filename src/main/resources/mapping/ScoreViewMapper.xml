<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.demo.mapper.ScoreViewMapper" >
  <resultMap id="BaseResultMap" type="com.example.demo.model.ScoreView" >
    <result column="scores" property="scores" jdbcType="INTEGER" />
    <result column="studentname" property="studentname" jdbcType="VARCHAR" />
    <result column="classname" property="classname" jdbcType="VARCHAR" />
    <result column="coursename" property="coursename" jdbcType="VARCHAR" />
  </resultMap>
  <insert id="insert" parameterType="com.example.demo.model.ScoreView" >
    insert into v (scores, studentname, classname, 
      coursename)
    values (#{scores,jdbcType=INTEGER}, #{studentname,jdbcType=VARCHAR}, #{classname,jdbcType=VARCHAR}, 
      #{coursename,jdbcType=VARCHAR})
  </insert>
  <insert id="insertSelective" parameterType="com.example.demo.model.ScoreView" >
    insert into v
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="scores != null" >
        scores,
      </if>
      <if test="studentname != null" >
        studentname,
      </if>
      <if test="classname != null" >
        classname,
      </if>
      <if test="coursename != null" >
        coursename,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="scores != null" >
        #{scores,jdbcType=INTEGER},
      </if>
      <if test="studentname != null" >
        #{studentname,jdbcType=VARCHAR},
      </if>
      <if test="classname != null" >
        #{classname,jdbcType=VARCHAR},
      </if>
      <if test="coursename != null" >
        #{coursename,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
</mapper>